[{
    "name": "default",
    "type": "postgres",
    "url": "$(grep DATABASE_URL .env | cut -d '=' -f2)",
    "ssl": {
        "rejectUnauthorized": false
    },
    "entities": ["dist/**/*.entity{ .ts,.js}"],
    "synchronize": false,
    "migrations": ["dist/migrations/*{.ts,.js}"],
    "migrationsTableName": "migrations_typeorm",
    "migrationsRun": true
}]